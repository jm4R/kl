@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Boost 1.61.0)
find_dependency(Microsoft.GSL)
if(@KL_ENABLE_YAML@)
    find_dependency(yaml-cpp 0.7)
endif()
if(@KL_ENABLE_JSON@)
    find_dependency(RapidJSON)
    if(NOT TARGET rapidjson)
        add_library(rapidjson ALIAS RapidJSON::RapidJSON)
    endif()
endif()

if(NOT TARGET kl::kl)
    include("${CMAKE_CURRENT_LIST_DIR}/klTarget.cmake")
endif()
if(NOT TARGET kl::json AND TARGET kl::kl-json)
    add_library(kl::json ALIAS kl::kl-json)
endif()
if(NOT TARGET kl::yaml AND TARGET kl::kl-yaml)
    add_library(kl::yaml ALIAS kl::kl-yaml)
endif()
